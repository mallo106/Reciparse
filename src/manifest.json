{
  "name": "Reciparse",
  "description": "Pulls recipe ingredients and instructions from the current web page",
  "version": "1.0.0.4",
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Reciparse",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "icons": {
    "18": "icons/baseline_fastfood_black_18dp.png",
    "36": "icons/baseline_fastfood_black_18dp_2x.png",
    "128": "icons/baseline_fastfood_black_18dp_128.png"
  },
  "manifest_version": 2
}
